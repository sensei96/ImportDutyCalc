<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5sp"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linearupper"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="5sp"
            tools:ignore="InvalidId">


            <TextView
                android:id="@+id/texthscode"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/overlock"
                android:text="HSCode  25172000"
                />

            <TextView
                android:id="@+id/textbm"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50sp"
                android:layout_weight="1"
                android:fontFamily="@font/overlock"
                android:gravity="center_horizontal"
                android:text=" BM  5.00"
                />

            <TextView
                android:id="@+id/textppn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2sp"
                android:layout_weight="1"
                android:fontFamily="@font/overlock"
                android:gravity="center_horizontal"
                android:text=" PPN  5.00"
                />

            <TextView
                android:id="@+id/textppnbm"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2sp"
                android:layout_weight="1"
                android:fontFamily="@font/overlock"
                android:gravity="center_horizontal"
                android:text=" PPNBM  5.00"
                />

            <TextView
                android:id="@+id/textpph"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2sp"
                android:layout_weight="1"
                android:fontFamily="@font/overlock"
                android:gravity="center_horizontal"
                android:text=" PPH  5.00"
                />
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/relativegaris1"
            android:layout_width="match_parent"
            android:layout_height="2sp"
            android:layout_below="@+id/linearupper"
            android:background="#575050"></RelativeLayout>

        <TextView
            android:id="@+id/textdesc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/relativegaris1"
            android:layout_marginTop="5sp"
            android:fontFamily="@font/overlock"
            android:gravity="center_horizontal"
            android:text=" DESKRIPSI \n GARAM; BELERANG; TANAH DAN BATU; BAHAN PLESTER, KAPUR DAN SEMEN" />

        <RelativeLayout
            android:id="@+id/relativegaris2"
            android:layout_width="match_parent"
            android:layout_height="2sp"
            android:layout_below="@+id/textdesc"
            android:layout_marginTop="5sp"
            android:background="#575050"></RelativeLayout>

        <TextView
            android:id="@+id/textbab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/relativegaris2"
            android:layout_marginTop="5sp"
            android:fontFamily="@font/overlock"
            android:gravity="center_horizontal"
            android:text=" BAB \n Batu kecil, gravel, batu pecah atau batu tumbuk, yang lazim digunakan untuk campuran beton, untuk mengeraskan jalan atau untuk rel kereta api atau pemberat lainnya, shingle dan flint, diolah dengan di"
            />

        <TextView
            android:id="@+id/textbabeng"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textbab"
            android:layout_marginTop="10sp"
            android:fontFamily="@font/overlock"
            android:gravity="center_horizontal"
            android:text=" BAB ENG \n Pebbles, gravel, broken or crushed stone, of a kind commonly used for concrete aggregates, for road metalling or for railway or other ballast, shingle and flint, whether or not heat-treated; macadam o"
            />

        <RelativeLayout
            android:id="@+id/relativegaris3"
            android:layout_width="match_parent"
            android:layout_height="2sp"
            android:layout_below="@+id/textbabeng"
            android:layout_marginTop="5sp"
            android:background="#575050"></RelativeLayout>

        <TextView
            android:id="@+id/textpost"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/relativegaris3"
            android:layout_marginTop="5sp"
            android:fontFamily="@font/overlock"
            android:gravity="center_horizontal"
            android:text=" POSTARIF \n - Makadam dari terak, sampah atau sisa buangan industri sejenis, bercampur dengan bahan tersebut dalam subpos 2517.10 maupun tidak"
            />

        <TextView
            android:id="@+id/textposten"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textpost"
            android:layout_marginTop="10sp"
            android:fontFamily="@font/overlock"
            android:gravity="center_horizontal"
            android:text=" POST TARIF ENG \n Macadam of slag, dross or similar industrial waste, whether or not incorporating the materials cited in subheading 2517.10"
            />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="1sp"
        android:layout_alignLeft="@+id/layoutklik"
        android:layout_alignStart="@+id/layoutklik"
        android:layout_below="@+id/layoutklik">
    </RelativeLayout>

    <com.google.android.gms.ads.AdView
        android:id="@+id/adView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        ads:adSize="BANNER"
        ads:adUnitId="@string/banner_ad_unit_id">
    </com.google.android.gms.ads.AdView>
</RelativeLayout>